logList=()

potentialLogList=("$(find /var/log -type f)")
presetRules=("$(ls /etc/logrotate.d)")

# Exceptions (most likely individual entries are provided):
exceptions=("fetchapply.log" "alternatives.log" "apt/term.log" "apt/history.log" "btmp" "ufw.log" "wtmp" "syslog" "mail.info" "mail.warn" "mail.err" "mail.log" "daemon.log" "kern.log" "auth.log" "user.log" "lpr.log" "cron.log" "debug" "messages" "ubuntu-advantage.log")

logListCounter=0
for logFile in ${potentialLogList[@]}
do
	matchFound="No"
	for rule in ${presetRules[@]}
	do
		if [ -n "$(echo $logFile | grep $rule)" ]
		then
			matchFound="Yes"
			break
		fi
	done

	if [ $matchFound == "No" ]
	then
		for rule in ${exceptions[@]}
		do
			if [ -n "$(echo $logFile | grep $rule)" ]
			then
				matchFound="Yes"
				break
			fi
		done
	fi

	if [ $matchFound == "No" ]
	then
		logList[$logListCounter]="$logFile"
		logListCounter=$((logListCounter+=1))
	fi
done
